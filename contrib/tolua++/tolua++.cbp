<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="tolua++" />
		<Option platforms="Windows;" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option platforms="Windows;" />
				<Option output="bin\Debug\tolua++" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Debug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add directory="lib\Debug" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option platforms="Windows;" />
				<Option output="bin\Release\tolua++" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add directory="lib\Release" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-O2" />
			<Add option="-Wall" />
			<Add option="-ansi" />
			<Add directory="include" />
			<Add directory="..\lua\include" />
		</Compiler>
		<Linker>
			<Add library="src\bin\toluabind.o" />
			<Add library="tolua++" />
			<Add library="..\lua\lua51.lib" />
			<Add library="..\lua\lua5.1.lib" />
			<Add directory="..\lua" />
		</Linker>
		<ExtraCommands>
			<Add after='cmd /c copy &quot;${TARGET_OUTPUT_FILE}&quot; &quot;bin\&quot;' />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="src\bin\tolua.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
