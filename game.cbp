<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Quipkit (game.exe)" />
		<Option platforms="Windows;" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="game-debug" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Debug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="game" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wmissing-include-dirs" />
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-std=c99" />
			<Add directory="contrib\lua\include" />
		</Compiler>
		<Linker>
			<Add library="contrib\lua\lua51.lib" />
			<Add library="shlwapi" />
			<Add directory="contrib\lua" />
		</Linker>
		<Unit filename="Makefile">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="README.textile">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="src\fs.h" />
		<Unit filename="src\fs_windows.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="src\game.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="src\options.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="src\options.h" />
		<Unit filename="test\glob.png">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="test\smile.png">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
