<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Quipkit Editor" />
		<Option platforms="Windows;" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin\Debug\edit" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Debug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin\Release\edit" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-std=c99" />
			<Add directory="contrib\lua\include" />
		</Compiler>
		<Linker>
			<Add library="contrib\lua\lua51.lib" />
			<Add library="contrib\lua\lua5.1.lib" />
			<Add directory="contrib\lua" />
		</Linker>
		<Unit filename="dev\init.lua">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="dev\lib\ltcl.lua">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="dev\lib\ltk.lua">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="dev\main.lua">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="edit.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
