<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="LuaGL (LuaGL.dll)" />
		<Option platforms="Windows;" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option platforms="Windows;" />
				<Option output="bin\Debug\LuaGL" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Debug\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option platforms="Windows;" />
				<Option output="bin\Release\LuaGL" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-O2" />
			<Add option="-Wall" />
			<Add directory="..\..\..\contrib\lua\include" />
		</Compiler>
		<Linker>
			<Add library="opengl32" />
			<Add library="..\..\..\contrib\lua\lua51.lib" />
			<Add library="..\..\..\contrib\lua\lua5.1.lib" />
			<Add directory="..\..\..\contrib\lua" />
		</Linker>
		<ExtraCommands>
			<Add after='cmd /c copy &quot;${TARGET_OUTPUT_FILE}&quot; .' />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="..\..\gl.lua">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="luagl.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="luagl.h" />
		<Unit filename="luagl_const.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="luagl_const.h" />
		<Unit filename="luagl_util.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="luagl_util.h" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
